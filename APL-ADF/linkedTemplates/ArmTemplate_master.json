{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "APL-ADF"
		},
		"AzureSqlDatabase_Source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase_Source'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=apldev.database.windows.net;Initial Catalog=aplsourcedev;User ID=apl_admin"
		},
		"Aplpracdev_Kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://aplpracdev-kv.vault.azure.net/"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://apldatalakesource.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "7085725b-f01d-4d02-ad1c-b1a8a6f44106"
		},
		"AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "a63d24dc-bd54-42d8-8fdb-0a8e791d502b"
		},
		"AzureDataLakeStorage_Sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://apldatalakedev.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage_Sink_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "7085725b-f01d-4d02-ad1c-b1a8a6f44106"
		},
		"AzureDataLakeStorage_Sink_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "a63d24dc-bd54-42d8-8fdb-0a8e791d502b"
		},
		"RestService_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airlabs.co/api/v9/airlines"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "APL-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase_Source_connectionString": {
						"value": "[parameters('AzureSqlDatabase_Source_connectionString')]"
					},
					"Aplpracdev_Kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('Aplpracdev_Kv_properties_typeProperties_baseUrl')]"
					},
					"RestService_source_properties_typeProperties_url": {
						"value": "[parameters('RestService_source_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeStorage_Sink_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_Sink_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage_Sink_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage_Sink_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage_Sink_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage_Sink_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}